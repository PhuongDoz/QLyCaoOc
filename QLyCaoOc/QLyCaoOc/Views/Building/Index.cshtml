@using QLyCaoOc.Models


@model IEnumerable<QLyCaoOc.Models.HOPDONG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}



<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table id="dynamic-table" class="table table-striped table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaHD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayLap)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TienCoc)
        </th>
        <th></th>
    </tr>

    @foreach (var hopdong in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => hopdong.MaHD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => hopdong.NgayLap)
        </td>
        <td>
            @Html.DisplayFor(modelItem => hopdong.TienCoc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = hopdong.MaHD }) |
            @Html.ActionLink("Details", "Details", new { id = hopdong.MaHD }) |
            @Html.ActionLink("Delete", "Delete", new { id = hopdong.MaHD })
        </td>
    </tr>
    }

</table>
